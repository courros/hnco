        },

        {
            "id": "jmp-10",
	    "opt": "-F 30 -t 10"
        },

        {
            "id": "djmp-5",
	    "opt": "-F 31 -t 5"
        },

        {
            "id": "djmp-10",
	    "opt": "-F 31 -t 10"
        },

        {
            "id": "fp-5",
	    "opt": "-F 40 -t 5"
        },

        {
            "id": "fp-10",
