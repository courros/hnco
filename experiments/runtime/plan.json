{
    "exec": "hnco",
    "opt": "--no-header --print-performance --map 1 --map-random -i 0 -b 0 --stop-on-maximum",

    "num_runs": 10,

    "results": "results",
    "report": "report",
    "graphics": "graphics",

    "parameter": {
        "id": "bv-size",
        "values": [ 100, 200, 300, 400, 500, 600, 700, 800, 900, 1000 ],
        "boxwidth": "(40)"
    },

    "functions": [

        {
            "id": "one-max",
	    "opt": "-F 0"
        },

        {
            "id": "leading-ones",
	    "opt": "-F 10"
        }

    ],

    "algorithms": [

        {
            "id": "ea-1p1",
            "title": "(1+1) EA",
	    "opt": "-A 300"
        },

        {
            "id": "rls",
            "title": "RLS",
	    "opt": "-A 100 --patience 0"
        }

    ]

}
